{
  "name": "angular-gettext-cli",
  "version": "1.0.7",
  "description": "A command line interface for angular gettools",
  "bin": {
    "angular-gettext-cli": "bin/gettext"
  },
  "scripts": {
    "test": "npm run test-extract && npm run test-compile",
    "test-extract": "./bin/gettext --files 'test/*.+(html|js)' --dest test/extract.pot --marker-name i18n",
    "test-compile": "./bin/gettext --compile --files 'test/*.po' --dest test/output.js --module gettext-test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/huston007/angular-gettext-cli.git"
  },
  "keywords": [
    "Angular"
  ],
  "author": "Andrey Skladchikov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huston007/angular-gettext-cli/issues"
  },
  "homepage": "https://github.com/huston007/angular-gettext-cli",
  "dependencies": {
    "angular-gettext-tools": "^2.1.11",
    "glob": "^6.0.4",
    "minimist": "^1.1.0"
  }
}
